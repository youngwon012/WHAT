import java.util.Scanner;

public class HA {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("야 숫자하나 입력해봐");
		int num = sc.nextInt();

		if(num<18)
			System.out.println("num이 18보다 작네");
		else
			System.out.println("num이 18보다 작지 않다.");

	}

}
