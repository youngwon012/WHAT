public class Dksl {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("배열의 크기는?");
		int 배열크기 = sc.nextInt();
		
		int[] 영원 = new int[배열크기];
		
		for(int i=0; i<영원.length; i++) {
			System.out.println("입력해바바");
			영원[i] = sc.nextInt();
		}
			
		System.out.println("정렬 전: ");
		for(int i=0;i<영원.length; i++) {
			System.out.println(" "+영원[i]);
		}
	}

}
