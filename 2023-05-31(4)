import random 

rn = random.randrange(1,101,1)
num = -1
t_cut = 0

print("Up & Down 게임시작")
print("------------------")
while ( rn != num ):

  num = int(input("1 ~ 100 숫자 : "))

  if(num >rn) :
    print("Down")
  elif(num < rn) :
    print("Up")

    t_cut +=1

print("------------------")
print(t_cut, "번 만에 정답")
